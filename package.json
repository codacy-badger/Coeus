{
	"name": "coeus",
	"version": "1.0.0",
	"description": "The God of Intelligence",
	"main": "index.js",
	"repository": "https://github.com/stevenselcuk/Coeus.git",
	"author": "Steven J. Selcuk",
	"license": "MIT",
	"scripts": {
		"dev": "NODE_ENV=development webpack --config ./webpack.config.dev.babel.js",
		"dev:debug": "NODE_ENV=development nodemon src/server.js --exec babel-node",
		"build": "yarn clean && babel -d ./build ./src -s && NODE_ENV=development node ./index.js",
		"deploy": "NODE_ENV=production webpack --config ./webpack.config.production.babel.js && pm2 start pm2.config.js",
		"start": "NODE_ENV=production node build/server.js",
		"seed": "NODE_ENV=development babel-node tools/seeder/seeder.js",
		"test": "ava ./test/**/*.js --color",
		"doc": "./node_modules/.bin/jsdoc -c jsdoc.json",
		"apidoc": "gulp apidoc",
		"test:watch": "jest --watchAll",
		"mocha": "mocha ./test/**/*.js",
		"report-coverage": "NODE_ENV=test nyc ./node_modules/.bin/mocha ./test/**/*.js && codecov -t c76b1eb3-ebac-415e-b58f-1bfdb7500f99",
		"unit": "jest \"src/.*\\.js$\" --coverage --forceExit",
		"e2e": "cross-env JEST_ENV=e2e jest \"src/.*\\.js$\" --forceExit",
		"clean": "rm -rf build && mkdir build && pm2 kill && pm2 flush",
		"_codecov": "codecov"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn mocha"
		}
	},
	"lint-staged": {
		"src/**/*.js": [
			"eslint --fix"
		],
		"src/**/*.{js,json,md}": [
			"prettier --write"
		]
	},
